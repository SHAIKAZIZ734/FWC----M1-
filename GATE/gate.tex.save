\begin{center} def logic_function(U, V, W, X):
    # First MUX
    $if U == 0 and V == 0:$
        mux1_out = 1  # Vcc is considered 1
    $elif U == 0 and V == 1:$
        mux1_out = 1  # Vcc is considered 1
    $elif U == 1 and V == 0:$
        mux1_out = 1  # Vcc is considered 1
    $ elif U == 1 and V == 1:$
        mux1_out = 0  # Ground is considered 0
    
    # Second MUX
    $if W == 0 and X == 0:$
        F = mux1_out
   $ elif W == 0 and X == 1:$
        F = mux1_out
   $ elif W == 1 and X == 0:$
        F = mux1_out
   $ elif W == 1 and X == 1:$
        F = 0  # Ground is considered 0
    
    return F

# Test cases
print(logic_function(0, 0, 0, 0))  # Output: 1
print(logic_function(0, 1, 0, 1))  # Output: 1
print(logic_function(1, 0, 1, 0))  # Output: 1
print(logic_function(1, 1, 1, 1))  # Output: 0
 
